{
  "name": "pomotron",
  "version": "1.0.2",
  "description": "Aplicativo para controle de tempo",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --all",
    "osx": "electron-packager app Pomotron --platform=darwin --arch=x64 --version=1.4.15",
    "win32": "electron-packager app Pomotron --platform=win32 --arch=ia32 --version=1.4.15",
    "win64": "electron-packager app Pomotron --platform=win32 --arch=x64 --version=1.4.15",
    "linux32": "electron-packager app Pomotron --platform=linux --arch=ia32 --version=1.4.15",
    "linux64": "electron-packager app pomotron --platform=linux --arch=x64 --version=1.4.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Silva01/pomotron.git"
  },
  "keywords": [
    "tempo",
    "pomodoro",
    "estudo",
    "concurso"
  ],
  "author": "Daniel Silva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Silva01/pomotron/issues"
  },
  "homepage": "https://github.com/Silva01/pomotron#readme",
  "dependencies": {
    "chartjs": "^0.3.24",
    "d3": "^4.8.0",
    "jquery": "~2.1.1",
    "materialize-css": "^0.98.2"
  }
}
